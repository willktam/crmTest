<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="StaffList" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="CRM_StaffList"/>
    <singularAlias>StaffList</singularAlias>
    <pluralAlias>StaffLists</pluralAlias>
    <iconStyleClass>fa fa-users</iconStyleClass>
    <bizKey expression="StaffList - {name}"/>
    <attributes>
    	<text required="true" name="name">
            <displayName>Name</displayName>
            <description><![CDATA[Your name]]></description>
            <length>100</length>
        </text>
    </attributes>
</document>