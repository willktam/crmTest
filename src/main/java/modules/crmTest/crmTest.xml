<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="http://www.skyve.org/xml/module" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" title="CrmTest" prototype="true" name="crmTest" xsi:schemaLocation="http://www.skyve.org/xml/module ../../schemas/module.xsd">
    <homeRef>list</homeRef>
    <homeDocument>Customer</homeDocument>
    <documents>
        <document ref="Customer"/>
        <document ref="Owner"/>
    </documents>
    <roles>
        <role name="Viewer">
            <description><![CDATA[Enough privileges to view CrmTest documents.]]></description>
            <privileges>
                <document name="Customer" permission="_R__C"/>
                <document name="Owner" permission="_R__C"/>
            </privileges>
        </role>
        <role name="Maintainer">
            <description><![CDATA[Enough privileges to create and edit CrmTest documents.]]></description>
            <privileges>
                <document name="Customer" permission="CRUDC"/>
                <document name="Owner" permission="CRUDC"/>
            </privileges>
        </role>
    </roles>
    <menu>
        <list document="Customer" name="Customers">
            <role name="Maintainer"/>
            <role name="Viewer"/>
        </list>
        <list document="Owner" name="Owners">
            <role name="Maintainer"/>
            <role name="Viewer"/>
        </list>
    </menu>
</module>