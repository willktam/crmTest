<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Owner" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="CRM_Owner"/>
    <singularAlias>Owner</singularAlias>
    <pluralAlias>Owners</pluralAlias>
    <iconStyleClass>fa fa-user</iconStyleClass>
    <bizKey expression="Owner - {name}"/>
    <attributes>
        <text required="true" name="name">
            <displayName>Name</displayName>
            <description><![CDATA[Owner name]]></description>
            <length>100</length>
        </text>
    </attributes>
</document>