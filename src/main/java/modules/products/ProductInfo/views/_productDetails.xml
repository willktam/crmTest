<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="_productDetails" title="Product Details" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
    <form border="true" borderTitle="Product Details">
        <column percentageWidth="30" responsiveWidth="4"/>
        <column/>
        <row>
            <item>
                <default binding="name"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="productId"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="description"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="validFrom"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="validTo"/>
            </item>
        </row>
    </form>
    <actions>
        <defaults/>
    </actions>
    <newParameters/>
</view>
