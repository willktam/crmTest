<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="ProductSales" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="PRO_ProductSales"/>
    <singularAlias>Product Sales</singularAlias>
    <pluralAlias>Product Saleses</pluralAlias>
    <iconStyleClass>fa fa-dollar</iconStyleClass>
    <bizKey expression="Product Sales - {product}"/>
    <attributes>
        <association required="true" type="aggregation" name="product">
            <displayName>Product</displayName>
            <documentName>ProductInfo</documentName>
        </association>
        <boolean required="true" name="active">
            <displayName>Active</displayName>
            <description><![CDATA[Is the sale of the product active]]></description>
        </boolean>
        <longInteger required="true" name="numerOfSales">
            <displayName>Numer of Sales</displayName>
            <description><![CDATA[Number of sales for the product]]></description>
        </longInteger>
    </attributes>
</document>