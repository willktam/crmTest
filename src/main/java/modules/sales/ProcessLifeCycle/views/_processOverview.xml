<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="_processOverview" title="Process Overview" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
    <form border="true" borderTitle="Process Overview">
        <column percentageWidth="30" responsiveWidth="4"/>
        <column/>
        <row>
        	<item>
        		<textField binding="processId" />
        	</item>
        </row>
        <row>
        	<item>
        		<lookupDescription descriptionBinding="field" binding="status" >
        			<onPickedHandlers>
        				<rerender clientValidation="false" />
        			</onPickedHandlers>
        		</lookupDescription>
        	</item>
        </row>
        <row>
        	<item>
        		<textField binding="startDate" />
        	</item>
        </row>
        <row>
        	<item>
        		<textField binding="endDate" />
        	</item>
        </row>
    </form>
    <actions>
        <defaults/>
    </actions>
    <newParameters/>
</view>