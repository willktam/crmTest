<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="_leadSummary" title="Lead Summary" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
    <form border="true" borderTitle="Lead Summary">
        <column percentageWidth="30" responsiveWidth="4"/>
        <column/>
        <row>
        	<item>
        		<textField binding="lead.bizKey" visible="leadSelected"/>
        	</item>
        </row>
       	<row>
       		<item>
       			<textField binding="lead.name" visible="leadSelected"/>
       		</item>
       	</row>
         <row>
         	<item>
         		<textField binding="lead.website" visible="leadSelected"/>
         	</item>
         </row>
         <row>
         	<item>
         		<textField binding="lead.contactDetails.bizKey" visible="leadSelected"/>
         	</item>
         </row>
         <row>
         	<item>
         		<textField binding="lead.leadType" visible="leadSelected"/>
         	</item>
         </row>
    </form>
    <actions>
        <defaults/>
    </actions>
    <newParameters/>
</view>