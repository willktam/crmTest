<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="edit" title="Invoice" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
    <form responsiveWidth="12" percentageWidth="60" border="true">
        <column percentageWidth="30" responsiveWidth="4"/>
        <column/>
        <row>
            <item>
                <default binding="invoiceId"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="name"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="order"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="invoiceStatus"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="description"/>
            </item>
        </row>
    </form>
    <actions>
        <defaults/>
    </actions>
    <newParameters/>
</view>
