<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Leads" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="STA_StaffList"/>
    <singularAlias>Lead</singularAlias>
    <pluralAlias>Leads</pluralAlias>
    <iconStyleClass>fa fa-phone-square</iconStyleClass>
    <bizKey expression="{contactDetails.lastName}"/>
    
    <attributes>
    	
    	<association required="true" type="aggregation" name="contactDetails">
    		<displayName>Contact Details</displayName>
    		<description>The contact details of the lead</description>
    		<documentName>ContactDetails</documentName>
    	</association>
        
    </attributes>
    
    <conditions>
		<condition name="Staff">
			<expression><![CDATA[isUserInRole("sales","Staff")]]></expression>
		</condition>
	</conditions>
</document>