<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="_manualInteraction" title="Manual Interaction" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
	<form border="true" >
        <column percentageWidth="60" responsiveWidth="8"/>
		<row>
            <item>
                <textField binding="interactions.title"/>
            </item>
        </row>
        <row>
        	<item>
        		<textField binding="interactions.description" />
        	</item>
        </row>
        <row>
        	<item>
        		<combo binding="interactions.type" />
        	</item>
        </row>
      	<row>
      		<item>
      			<button actionName="save" />
      		</item>
      	</row>
	</form>
</view>