<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="edit" title="Contact Details" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
    <component name="_flowbar" />
    <tabPane selectedTabIndexBinding="selectedTab">
    	<tab title="Summary">
			<vbox responsiveWidth="7" percentageWidth="70">
				<component name="_contactDetails" />
				<component name="_addressDetails" />
			</vbox>	
    	</tab>
    	<tab title="Interactions" visible="persisted">
			<component name="_interactions" />
    	</tab>
    </tabPane>
    
    <actions>
        <defaults/>
	    <action className="AddInteraction" displayName="Add" inActionPanel="false" iconStyleClass="fa fa-plus" />
    </actions>
    <newParameters/>
</view>
