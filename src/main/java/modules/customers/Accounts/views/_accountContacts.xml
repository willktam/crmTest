<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="_accountContacts" title="Account Contacts" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
	<form border="true" borderTitle="Account Contacts">
        <column percentageWidth="30" responsiveWidth="4"/>
        <column/>
        
        <row>
            <item>
                <default binding="primaryContact"/>
            </item>
        </row>
        <row>
        	<item>
        		<default binding="primaryContact.firstName" />
        	</item>
        </row>
        <row>
        	<item>
        		<default binding="primaryContact.lastName" />
        	</item>
        </row>
        <row>
        	<item>
        		<default binding="primaryContact.email" />
        	</item>
        </row>
        <row>
        	<item>
        		<default binding="primaryContact.businessNumber" />
        	</item>
        </row>
    </form>
</view>