<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="edit" title="Interaction" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
    <form responsiveWidth="12" percentageWidth="60" border="true">
        <column percentageWidth="30" responsiveWidth="4"/>
        <column/>
        <row>
            <item>
                <default binding="title"/>
            </item>
        </row>
        <row>
            <item>
                <textField binding="user.contact.name" editable="false" />
            </item>
        </row>
        <row>
            <item>
                <default binding="description"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="type"/>
            </item>
        </row>
        <row>
            <item>
                <textField binding="interactionTime" editable="false" />
            </item>
        </row>
        <row>
            <item>
                <default binding="contact"/>
            </item>
        </row>
    </form>
    <actions>
        <defaults/>
    </actions>
    <newParameters/>
</view>
