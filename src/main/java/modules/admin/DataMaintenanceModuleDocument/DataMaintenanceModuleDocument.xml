<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<document name="DataMaintenanceModuleDocument"
			xmlns="http://www.skyve.org/xml/document"
			xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<singularAlias>Module Document</singularAlias>
	<pluralAlias>Module Documents</pluralAlias>
	<attributes>
		<boolean name="include">
			<displayName>Include</displayName>
		</boolean>
		<text name="moduleName" required="true">
			<displayName>Module Name</displayName>
			<description>The name of the module.</description>
			<length>500</length>
		</text>
		<text name="documentName" required="true">
			<displayName>Document Name</displayName>
			<description>The name of the document.</description>
			<length>500</length>
		</text>
		<text name="modDocName" required="true">
			<displayName>Document Name</displayName>
			<description>The name of the document.</description>
			<length>500</length>
		</text>
	</attributes>
</document>
